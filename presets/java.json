{
  "stack": "java",
  "name": "Java (Spring Boot)",
  "defaultPort": 8080,
  "runtimeVersion": "21",
  "installCommand": "mvn -B dependency:go-offline",
  "lintCommand": "mvn -B checkstyle:check --no-transfer-progress || true",
  "testCommand": "mvn -B verify --no-transfer-progress",
  "buildCommand": "mvn -B package -DskipTests --no-transfer-progress",
  "startCommand": "java -jar target/app.jar",
  "dockerImageBaseName": "java-app",
  "healthPath": "/actuator/health"
}